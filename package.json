{
  "name": "exercise-mcp-server",
  "version": "1.0.0",
  "description": "Web-based MCP server for exercise database with 1300+ exercises via HTTP API",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:web": "npm run build && node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:web": "tsx src/index.ts",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mcp",
    "exercise",
    "fitness",
    "workout",
    "claude"
  ],
  "author": "Fittality Exercise MCP Server",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.2",
    "jest": "^29.7.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
